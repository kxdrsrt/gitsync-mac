# Custom Include List (do not modify this comment)
!~/.*
!~/.config
!/Library/Application Support
!/Library/PreferencePanes
!/Library/Preferences
!/System/Library/PreferenceBundles
!/System/Library/PreferenceManifests
!/System/Library/PreferencePanes

# Custom Exclude List (do not modify this comment)

## Exclusions from the macOS root directory
/Library/Application Support/ApplePushService
/Library/Application Support/BTServer
/Library/Application Support/iLifeMediaBrowser
/Library/Application Support/Microsoft
/Library/Application Support/ProApps
/Library/Application Support/Ryujinx
/Library/Application Support/com.apple.idleassetsd
/Library/Application Support/MobileSync/Backup
/Library/Containers/com.apple.ipswdownloadservice
/Library/Application Support/Dock/desktoppicture.db
/Library/Application Support/com.apple.sharedfilelist/com.apple.LSSharedFileList.ApplicationRecentDocuments/com.apple.systempreferences.sfl2
/Library/Preferences/com.apple.desktop.plist

## Exclusions from the user's home directory
*/cache/*
*/Cache/*
*/crashpad/*
*/history/*
*/History/*
*/logs/*
*/Logs/*
*/ParentalControls/*
.DS_Store
.Spotlight-V100
.Trash
.Trashes
.VolumeIcon.icns
.AppleDouble
.fseventsd
*.cache
*.crash*
*.history
*.log
*report*

# Exclude specific user directories
~/.config/raycast
~/.ssh
~/.vscode

# Exclude specific system and application directories
~/Applications
~/CloudDocs/*
~/Desktop
~/Documents
~/Downloads
~/Library
~/Movies
~/Music
~/Parallels Applications
~/Parallels Folders
~/Pictures
~/Public
~/Sites
~/Templates
~/Videos
~/WebCatalog Apps
~/Workspace

# Exclude iCloud preferences and directories
~/Library/Mobile Documents/*
~/Library/Preferences/com.apple.iCloudHelper.plist
~/Library/Preferences/com.apple.iCloudDrive.plist
~/Library/Preferences/MobileMeAccounts.plist

# Exclude password and keychain related directories and files
~/Library/Application Support/Keychains
~/Library/Keychains/*
~/Library/Preferences/com.apple.keychainaccess.plist

# Git-specific ignore patterns
node_modules/
